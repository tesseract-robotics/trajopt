<?xml version="1.0"?>
<package format="2">
  <name>tesseract_ros</name>
  <version>0.1.0</version>
  <description>The tesseract_ros package</description>
  <maintainer email="levi.armstrong@swri.org">Levi Armstrong</maintainer>
  <license>Apache 2.0</license>
  <license>BSD</license>
  <author>Levi Armstrong</author>
  <author>John Schulman</author>

  <buildtool_depend>catkin</buildtool_depend>
  <depend>roscpp</depend>
  <depend>urdf</depend>
  <depend>srdfdom</depend>
  <depend>geometric_shapes</depend>
  <depend>trajopt_ext</depend>
  <depend>kdl_parser</depend>
  <depend>eigen_conversions</depend>
  <depend>tesseract_core</depend>
  <depend>tesseract_msgs</depend>
  <depend>pluginlib</depend>

  <!-- The export tag contains other, unspecified, tags -->
  <export>
    <tesseract_ros plugin="${prefix}/tesseract_bullet_env_plugin_description.xml"/>
    <tesseract_ros plugin="${prefix}/tesseract_bullet_env_singleton_plugin_description.xml"/>
  </export>
</package>
