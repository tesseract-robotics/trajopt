            <geometry>
              <box size="1 0.722 0.050"/>
            </geometry>
            <material name="yellow"/>
          </collision>
        </link>
        <link name="${prefix}end_effector">
          <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
              <mesh filename="package://trajopt_examples/meshes/car_seat/visual/end_effector_simple.stl"/>
            </geometry>
            <material name="gray"/>
          </visual>
          <collision>
            <origin ryp="0 0 0" xyz="0 0 0"/>
            <geometry>
              <mesh filename="package://trajopt_examples/meshes/car_seat/visual/end_effector_simple.stl"/>
            </geometry>
            <material name="yellow"/>
