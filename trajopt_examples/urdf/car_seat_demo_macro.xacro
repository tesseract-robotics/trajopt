        <link name="${prefix}fence">
          <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
              <mesh filename="package://trajopt_examples/meshes/car_seat/visual/fence_structure.dae"/>
            </geometry>
            <material name="gray"/>
          </visual>
          <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
              <mesh filename="package://trajopt_examples/meshes/car_seat/visual/fence_mesh.dae"/>
            </geometry>
            <material name="gray"/>
          </visual>
          <collision>
            <origin rpy="0 0 0" xyz="-2.42 -2.250 0.86"/>
            <geometry>
              <box size="8.840 0.040 1.720"/>
            </geometry>
            <material name="yellow"/>
          </collision>
        </link>
        <link name="${prefix}conveyor">
          <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
              <mesh filename="package://trajopt_examples/meshes/car_seat/visual/conveyor.dae"/>
            </geometry>
            <material name="gray"/>
          </visual>
          <collision>
            <origin rpy="0 0 0" xyz="0 0 0.215"/>
            <geometry>
              <box size="2.995 1.320 0.430"/>
            </geometry>
            <material name="yellow"/>
          </collision>
        </link>
        <link name="${prefix}rail">
          <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
              <mesh filename="package://trajopt_examples/meshes/car_seat/visual/rail_structure.dae"/>
            </geometry>
            <material name="gray"/>
          </visual>
          <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
              <mesh filename="package://trajopt_examples/meshes/car_seat/visual/rail_feet.dae"/>
            </geometry>
            <material name="gray"/>
          </visual>
          <collision>
            <origin rpy = "0 0 0" xyz="-2.900 -1 0.125"/>
            <geometry>
              <box size="6.300 2.722 0.25"/>
            </geometry>
            <material name="yellow"/>
          </collision>
        </link>
        <link name="${prefix}carriage">
          <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
